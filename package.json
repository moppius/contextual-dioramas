    "event-hooks-webpack-plugin": "^2.1.6",
    "gh-pages": "^3.0.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "4.3.0",
    "indent-string": "^4.0.0",
    "raw-loader": "^4.0.1",
    "react-dev-utils": "^10.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
